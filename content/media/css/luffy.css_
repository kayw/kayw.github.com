#lf-links {
  z-index: 1001;
  position: fixed;
  left: 0;
  top: 160px;
  margin-left: -5px;
  background-color: rgba(255,255,255,0.5);
  padding: 0.7em 7px 0.7em 8px;
  border: 1px solid #929292;
  font: bold 0.875em futura-pt, 'Droid Sans', sans-serif, 'Lucida Grande', 'Lucida Sans Unicode', 'Arial Unicode MS';
  border-radius: 3px;
}
.no-rgba #lf-links {
  background: url("../images/l/translucent.png");
}
#lf-links p {
  color: #888;
}
#lf-links li {
  padding-left: 1em;
}
#lf-links li a {
  text-decoration: none;
  color: #21519C;
}
#lf-links li a:hover {
  color: #CC0000;
}
#lf-links .lf-link {
  position: relative;
}
#lf-links .lf-link .lf-link-details {
  position: absolute;
  display: block;
  visibility: hidden;
  transition: all 0.2s ease-in-out;
  top: -8px;
  left: 0;
}
#lf-links .lf-link .lf-link-inner {
  position: relative;
  margin-left: 15px;
  width: 12em;
  background-color: #F4F4F4;
  border: 1px solid #BEBEBE;
  padding: 0.7em 7px 0.7em 8px;
  border-radius: 3px;
}
#lf-links .lf-link .lf-sprite-arrow-left {
  position: absolute;
  z-index: 1001;
  left: -5px;
  top: 10px;
}
#lf-links .lf-link:hover .lf-link-details {
  left: 16px;
  top: -8px;
  visibility: visible;
}
@media only screen and (max-width: 768px) {
  #lf-links {
    position: static;
    background-color: rgba(200,200,200,0.6);
    width: 100%;
    padding-left: 40px;
    margin: 0;
    box-sizing: border-box;
    transition: none 0 ease-in-out;
  }
  #lf-links .lf-link {
    display: inline-block;
    zoom: 1;
    *display: inline;
  }
  #lf-links .lf-link .lf-link-details,
  #lf-links .lf-link .lf-link-inner {
    position: static;
    display: inline;
    visibility: visible;
    background: inherit;
    padding: 0;
    margin: 0;
    width: auto;
    border: none;
  }
  #lf-links .lf-sprite-arrow-left {
    display: none;
  }
  #lf-links .lf-link-details,
  #lf-links .lf-link-inner {
    display: inline;
    background: inherit;
  }
  #lf-links .lf-link-icon {
    vertical-align: middle;
    margin-right: 0.5em;
  }
  #lf-links p {
    display: inline;
    color: #EEE;
  }
  #lf-links ul {
    display: inline;
    padding-right: 2em;
  }
  #lf-links li {
    display: inline;
    padding-left: 0.2em;
    padding-right: 0.5em;
  }
  #lf-links li:before {
    color: white;
    content: "â ";
  }
}
@media only screen and (max-width: 480px) {
  #lf-links {
    padding-left: 0.5em;
  }
  #lf-links p {
    display: none;
  }
}
