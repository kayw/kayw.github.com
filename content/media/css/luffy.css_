/* Import from http://vincent.bernat.im/*/
#lf-disqus {
	text-align: center;
	margin: 1em auto;
	padding: 0.6em 0 0.4em 0;
	font: bold 1em ;
	text-shadow: 0 1px 1px #f3f6fc;
	background-color: #b3cbef;
	border-radius: 3px;
	cursor: pointer;
	display: none;
}
#lf-disqus:hover {
	color: red;
}
#lf-page #disqus_thread {
	margin-top: 3em;
	padding: 0.6em;
}
#lf-page #disqus_thread a {
	border-bottom: none;
}
#lf-page #disqus_thread #dsq-content .dsq-moderator .dsq-avatar img {
	border: 2px solid #58ACFA;
}
#lf-page #disqus_thread #dsq-comment-header {
	font-family: ;
}
#lf-pages {
	width: 46em;
}
#lf-pages #lf-page {
	width: 42em;
}
#lf-pages {
	min-height: 250px;
	margin: 2em auto;
	overflow: hidden;
}
#lf-pages #lf-page-1,
#lf-pages #lf-page-2,
#lf-pages #lf-page {
	background: #F9F9F9;
	border: solid 1px #EEE;
	box-shadow: 0 0 5px rgba(0,0,0,0.2);
}
#lf-pages #lf-page {
	overflow: hidden;
	padding: 1.625em 1em;
	margin: 5px;
}
#lf-page-1,
#lf-page-2 {
	display: none;
}
.csstransforms #lf-page-1,
.csstransforms #lf-page-2 {
	max-height: 1024px;
	position: absolute;
	margin-top: 5px;
}
.csstransforms #lf-page-1 {
	margin-left: -5px;
	z-index: -2;
	transform: rotate(-1deg);
	-webkit-backface-visibility: hidden;
}
.csstransforms #lf-page-2 {
	margin-left: 5px;
	z-index: -1;
	transform: rotate(2deg);
	-webkit-backface-visibility: hidden;
}
#lf-scrolling-header {
	color: #999;
	position: fixed;
	left: 5px;
	z-index: -5;
	text-align: right;
	display: none;
	font: bold 1.5em futura-pt, 'Droid Sans', sans-serif, 'Lucida Grande', 'Lucida Sans Unicode', 'Arial Unicode MS';
	text-shadow: 0px 1px 1px #FFF;
	overflow: hidden;
}
@media only screen and (max-width: 54em) {
	#lf-pages {
		width: 100% !important;
	}
	#lf-pages #lf-page-1,
	#lf-pages #lf-page-2 {
		display: none !important;
	}
	#lf-pages #lf-page {
		padding: 2em;
		margin: 0;
	}
}
@media only screen and (max-width: 42em) {
	#lf-pages #lf-page {
		padding: 1.625em 0.5%;
		width: 99% !important;
	}
}

body {
  font: normal 100% ff-tisa-web-pro, 'Droid Serif', Georgia, serif, 'Lucida Grande', 'Lucida Sans Unicode', 'Arial Unicode MS';
}
article {
  padding: 0 2em;
}
#lf-page {
  color: #2C2C2C;
  line-height: 1.625;
}
#lf-page header {
  padding-bottom: 1.625em;
  margin: 3.25em 1em;
  font-weight: bold;
  font-family: 'Source Sans Pro','Droid Sans',sans-serif, 'Lucida Grande', 'Lucida Sans Unicode', 'Arial Unicode MS';
}
#lf-page header h1 {
  color: #21519C;
  font-size: 2.625em;
  margin: 0;
}
#lf-page header h2 {
  text-align: right;
  color: #6F6F6F;
  font-size: 2.125em;
  margin: 0;
}
#lf-page time {
  color: #6F6F6F;
}
#lf-page p {
  margin: 1.625em 0 0 0;
  word-spacing: 0.25em;
}
#lf-page a {
  color: #21519C;
  text-decoration: none;
}
#lf-page a:hover {
  color: #4a82d9;
  border-bottom: 1px solid #4a82d9;
}
#lf-page em {
  font-style: italic;
}
#lf-page b,
#lf-page strong {
  font-style: italic;
  font-weight: normal;
  color: #B80F28;
}
#lf-page del {
  text-decoration: line-through;
  color: #666;
}
#lf-page ins {
  font-style: italic;
  text-decoration: none;
}
#lf-page abbr {
  cursor: help;
  border-bottom: 1px dashed #b3cbef;
}
#lf-page hr {
  background:url('../images/l/sprite/hr.png') no-repeat 0px 0px;width:300px; height:16px;
  display: block;
  border: none;
  margin: 3.25em auto;
}
#lf-page h1,
#lf-page h2,
#lf-page h3,
#lf-page h4 {
  color: #21519C;
  font-family: 'Source Sans Pro','Droid Sans',sans-serif, 'Lucida Grande', 'Lucida Sans Unicode', 'Arial Unicode MS';
  font-weight: bold;
}
#lf-page h1 {
  font-size: 2.125em;
  margin: 1.52941176471em 0 0.764705882353em;
}
#lf-page h2 {
  font-size: 1.625em;
  margin: 2em 0 1em;
}
#lf-page h3 {
  font-size: 1.312em;
  margin: 2.47713414634em 0 1.23856707317em;
}
#lf-page code {
  background: #dee8f8;
  background: rgba(221,232,248,0.6);
  border-radius: 3px;
  border: 1px solid #b3cbef;
  padding: 0 2px;
}
#lf-page sup {
  font-size: 0.7em;
  vertical-align: baseline;
  position: relative;
  top: -0.7em;
}
#lf-page table {
  overflow: auto;
  width: 92%;
  margin: 1.625em auto;
  border-spacing: 2px;
  border-color: gray;
  border-collapse: separate;
  vertical-align: middle;
}
#lf-page table th {
  padding: 0.5em;
  text-align: left;
  background-color: #21519C;
  color: white;
  background-image: -webkit-gradient(linear,center bottom,left bottom,from(#21519C),to(#255cb1));
  background-image: linear-gradient(center bottom,#21519C,#255cb1);
}
#lf-page table th a {
  color: #9ebceb;
}
#lf-page table th:first-child {
  border-radius: 4px 0 0 0;
}
#lf-page table th:last-child {
  border-radius: 0 4px 0 0;
}
#lf-page table tbody td {
  padding: 0.2em 0.5em;
}
#lf-page table tbody tr {
  background: #ffffff;
  background: rgba(255,255,255,0.6);
  color: #444;
}
#lf-page table tbody tr:nth-child(odd) {
  background: #dee8f8;
  background: rgba(221,232,248,0.6);
}
#lf-page table tbody tr:hover {
  color: black;
}
#lf-page blockquote {
  font-style: italic;
}
#lf-page blockquote em {
  font-style: normal;
}
#lf-page article blockquote {
  margin: 1.625em 1em 1.625em 1.5em;
  padding: 0 1.5em 1.625em;
  background: #c9d9f3;
  background: rgba(200,217,243,0.6);
}
#lf-page article blockquote:before {
  display: block;
  font-size: 4.5em;
  line-height: 0.888;
  height: 0.888em;
  overflow: hidden;
  content: 'Â';
  color: #21519C;
  margin-left: -0.555em;
  margin-bottom: -0.888em;
  font-family: Baskerville, 'Goudy Old Style', 'Goudy Bookletter 1911', Palatino, 'Palatino Linotype', 'URW Palladio L', 'Cochin', 'Book Antiqua', 'Apple Chancery', 'Constantia', 'Noticia Text', 'Droid Serif', Georgia, serif, 'Lucida Grande', 'Lucida Sans Unicode', 'Arial Unicode MS';
}
#lf-page article blockquote:lang(en):before {
  content: 'â';
  font-family: 'Source Sans Pro','Droid Sans',sans-serif, 'Lucida Grande', 'Lucida Sans Unicode', 'Arial Unicode MS';
}
.dsq-comment-message blockquote {
  margin: 1.625em 26px;
}
#lf-page .footnote {
  font-size: 0.75em;
}
#lf-page .footnote p {
  margin: 2.16666666667em 0 0 0;
}
article ul,
.dsq-comment-message ul {
  margin-left: 3em;
  list-style-type: square;
}
article ol,
.dsq-comment-message ol {
  list-style-type: decimal;
  padding-left: 40px;
}
article dl dt,
.dsq-comment-message dl dt {
  font-style: italic;
  color: #B80F28;
  margin: 0 3%;
}
article dl dt:before,
.dsq-comment-message dl dt:before {
  content: "Ã¢ï¿½ï¿½Ã ";
}
article dl dd,
